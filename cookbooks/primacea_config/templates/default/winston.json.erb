{
	"transports": [
		{
			"transport": "Loggly"
			,"options": {
				"subdomain": "<%= @subdomain %>",
				"inputToken": "<%= @inputToken %>",
				"auth": {
					"username": "<%= @username %>",
					"password": "<%= @password %>"
				}
			}
		}
		,{
			"transport": "File"
			,"options": {
				"filename": "<%= @logPath %>",
				"handleExceptions": true
			}
		}
	] 
}
